		'name' : '홍길동',
		'phone' : '010-1111-2222',
		'addr' : '경기도 광명시',
		'info' : function() {
			alert('name : ' + this.name + '\n')
			alert('name : ' + this.phone + '\n')
			alert('name : ' + this.addr + '\n')
		}
	}
	
	person.info()
</script>


