	 */

	let person = {
		'name' : '홍길동',
		'phone' : '010-1111-2222',
		'addr' : '경기도 광명시'
	//toString과 비슷한 정보임, 내가 원하는 정보만 stringify가 이루어짐
	/* 		toJSON : function() {
	 return {
	 'name' : this.name,
	 'phone' : this.phone
	 }
	 } */
	}
	/* console.log(person)
	console.log(JSON.stringify(person))
	alert(person)
	alert(JSON.stringify(person)) */

	let strPerson = JSON.stringify(person)
	console.log(strPerson)
	document.write(strPerson + '<br>')
	let person2 = JSON.parse(strPerson)
	console.log(person2)
	
	
	let str = "{name : '홍길동' ,phone:'010-1111-2222',addr:'경기도 광명시'}"
	
