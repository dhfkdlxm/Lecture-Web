<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">


	//깊은 복사
	function clone(obj) {
		let object = {}
		for ( let key in obj) {
			object[key] = obj[key]
		}
		return object
	}

	let person = {
		name : '홍길동',
		phone : '010-1111-2222'
	}
	//얖은 복	사
	//let person2 = person
	let person2 = clone(person)
	person2.phone = '010-2222-3333'

	console.log(person)
	console.log(person2)
</script>

</head>
<body>

</body>
</html>