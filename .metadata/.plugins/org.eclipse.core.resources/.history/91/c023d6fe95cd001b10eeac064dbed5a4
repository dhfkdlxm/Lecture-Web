<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
	let nums = [ 10, 20, 30, 40, 50 ]
	document.write('nums : ' + nums + '<br>')

	/*
	let copyNums = []
	for(let num of nums){
		copyNums.push(num + 100)
	}*/
	let copyNums = nums.map(function(element, index) {
		if (index != 2)
			return element + 100
		else
			return element
	})

	document.write('copyNums : ' + copyNums + '<br>')

	nums = [ 5, 23, 1, 676, 34, 67, 8, 34, 12, 56 ]

	let evenNums = nums.filter(function(element, index, array) {
		return element % 2 == 0
	})
	let evenNums2 = nums.map(function(element, index) {
		return element % 2 == 0
	})
	document.write('nums : ' + nums + '<br>')
	document.write('evenNums : ' + evenNums + '<br>')
	document.write('evenNums2 : ' + evenNums2 + '<br>')

	//every : 모든 조건을 만족하는 집합
	let bool = nums.every(function(element) {
		return element > 0
	})
	document.write('양의 정수 집합 판단 : ' + bool + '<br>')

	//some : 내가원하는 조건 한개 이상 존재하면 true
	bool = nums.some(function() {
		return element % 2 == 0
	})
	document.write('짝수 포함 여부 : ' + bool + '<br>')
</script>
</head>
<body>

</body>
</html>