<script type="text/javascript">
	/* 
	 * 	 JSON.parse() 		: 문자열을 자바스크립트 객체 변환
	 JSON.stringify()    : 자바스크립트 객체를 문자열로 변환
	 */

	let person = {
		'name' : '홍길동',
		'phone' : '010-1111-2222',
		'addr' : '경기도 광명시',
		
		toJSON : function(){
			return {
				'name' : phone.name,
				'phone' : this.phone
			}
		}
	}
	/* console.log(person)
	console.log(JSON.stringify(person))
	alert(person)
	alert(JSON.stringify(person)) */
	
	let strPerson = JSON.stringify(person)
	console.log(strPerson)
	document.write(strPerson + '<br>')
	
	
</script>
