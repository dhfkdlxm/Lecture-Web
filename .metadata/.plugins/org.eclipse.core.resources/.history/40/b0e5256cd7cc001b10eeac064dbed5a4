<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
	function makePerson(name, phone, addr) {
		let person = {
			'name' : name,
			'phone' : phone,
			'addr' : addr,
			'info' : function() {
				let msg = ''
				for(let key in this){
					msg += key + ' : '  + this[key] + '\n'
				}
			}
		}
		return person
	}
</script>

</head>
<body>

</body>
</html>