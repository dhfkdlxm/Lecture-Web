<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
	let books = [ '피터팬' , '신데렐라',  '콩쥐팥쥐', '백설공주']

	documnet.write('배열 출력 1번')
	for (var i = 0; i < books.length; i++) {
		document.write(books[i] + "&nbsp;&nbsp;&nbsp;")
	}
	document.write('<br>')
	documnet.write('배열 출력 2번')
	for(let i in books){
		document.write(books[i] + "&nbsp;&nbsp;&nbsp;")
	}
	document.write('<br>')
	documnet.write('배열 출력 3번')
	for(let book of books){
		document.write(book + "&nbsp;&nbsp;&nbsp;")
	}
	document.write('<br>')
	
	
	books.forEach(function(){
		console.log(arguments)
	})
	
	documnet.write('배열 출력 4번')
	books.forEach(function(element){
		document.write(element + "&nbsp;&nbsp;&nbsp;")
	})
	document.write('<br>')
	
	let nums = [12,6,25,63,4,32,18,4,55,35]
	document.write('nums : ' + nums + '<br>' )
	nums.sort()
	//그냥 sort()를 하게 되면 문자열 비교가됨
	document.write('sort() : nums =>'  + nums + '<br>')
	nums.sort(function(left, right){
		console.log(arguments)
		return left-right
	})
	document.write('오름차순 sort() : nums =>'  + nums + '<br>')
	
	nums.sort(function(left, right){
		return right-left
	})
	document.write('내림차순 sort() : nums =>'  + nums + '<br>')
	
</script>
</head>
