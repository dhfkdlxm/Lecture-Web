	let inputNum = prompt("10~100사이의 숫자를 입력하세요!")
	console.log(inputNum)

	for (var i = 1; i < inputNum + 1; i++) {
		if (i / 10 % 3 == 0 || i % 10 % 3 == 0) {
			alert('짝')
		} else {
			alert(i)
		}

	}

	/*  
