<script type="text/javascript">
	let person = {
		name : '홍길동',
		'phone' : '010-1111-2222'
	}

	console.log(person)
	alert('name : ' + person.name + ', phone : ' + person.phone)
	alert('name : ' + person['name'])

	alert('멤버변수 name 존재여부 : ' + ('name' in person))
	alert('멤버변수 phone 존재여부 : ' + ('phone' in person))
	alert('멤버변수 address 존재여부 : ' + ('address' in person))
	
	person.addr = '경기도 광명시'
</script>
