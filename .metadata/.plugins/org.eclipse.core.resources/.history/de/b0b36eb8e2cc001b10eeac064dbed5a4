<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
	let array = [ 1, 2, 3, 4, 5 ]
	//let array2 = array //얕은 복사

	//깊은 복사 1)
	let array2 = []
	for ( let i in array) {
		array2[i] = array[i]
	}
	
	//깊은 복사 2) ...array 전개 연산자 
	let array3 = [...array] //[...array] = [1,2,3,4,5]

	console.log('array : ' + array)
	console.log('array2 : ' + array2)

	array2[0] = 100
	console.log('array : ' + array)
	console.log('array2 : ' + array2)
	console.log('array3 : ' + array3)
</script>

</head>
<body>

</body>
</html>