	window.onload = function() {
		let h1Tag = document.createElement('h1')
		//document.createElements() : tag를 만들어주는 역할
		let text = document.createTextNode('hello')
		//document.createTextNode() : text를 입력해주는 역할
		console.log(h1Tag)
		console.log(text)
	}
</script>
