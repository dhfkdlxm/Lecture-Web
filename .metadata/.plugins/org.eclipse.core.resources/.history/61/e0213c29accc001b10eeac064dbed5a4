<script type="text/javascript">
	testFunction()//호이스팅 발생
	function testFunction(){
		alert('testFunction() 호출 ...')
	}
	
	
   function testFunction01(data){
	   alert('data : ' + data)
   }

   function testFunction02(a,b){
	   alert(`a : ${ a }, b : ${ b } `)
   }
   //오버로딩 지원되지 않음! 
   // 덮어 쓰기가 지원됨!
	testFunction02('A', 10)	
   testFunction01(123)
	
   function add(a, b){
	   return a+b
   }
   let sum = add(10, 20)
   alert('총합 : ' + sum)
   
   
	

</script>
