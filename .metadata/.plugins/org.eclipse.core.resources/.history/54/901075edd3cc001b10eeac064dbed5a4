<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
	let person = {
		'name' : '홍길동',
		'phone' : '010-1111-2222',
		'addr' : '경기도 광명시',
		'info' : function() {
			alert('name : ' + this.name + '\n' + 'phone : ' + this.phone + '\n'
					+ 'addr : ' + this.addr + '\n')
		}
	}
	//this키워드 : 자기 json문법안에서 자기 자신의 변수를 호출할ㄸ ㅐ사용(생략불가능)

	//this 쓰기가 귀찮으면?
	let person = {
		'name' : '홍길동',
		'phone' : '010-1111-2222',
		'addr' : '경기도 광명시',
		'info' : function() {
			let msg = ' '
			with (this) {
				msg += 'name : ' + name + '\n'
				msg += 'phone : ' + phone + '\n'
				msg += 'addr : ' + addr + '\n'
			}
			alert(msg)
			//alert('name : ' + this.name + '\n' + 'phone : ' + this.phone + '\n' + 'addr : ' + this.addr + '\n')
		}
	}
	person.info()
</script>


</head>
