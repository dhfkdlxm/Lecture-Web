<script type="text/javascript">
	$(document).ready(function() {
		//애니매이션 효과를 낼수있음 animiate(어떤효과?,몇초에 걸쳐서?)
/* 		$('div').animate({
			'width' : '+=200px',
			'height' : '200px',
			left : '100px',
			top : '50px',
			opacity : '0.6'
		}, 2000) */
		
		$('div').animate({
			width:'200px'
		})
		$('div').animate({
			left : '100px',
			opacity : '0.3'
		}, 2000)		
		$('div').animate({
			width : '100px',
			height : '200px',
			opacity : '0.7'
		}, 3000)
		$('div').animate({
			height : '100px',
			top: '100px',
			left : '0px',
			opacity : '1'
		}, 3000)
		
		
	})
</script>
