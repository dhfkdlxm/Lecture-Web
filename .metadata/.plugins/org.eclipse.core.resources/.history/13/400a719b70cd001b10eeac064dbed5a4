<script type="text/javascript">
	function Person(name, phone, addr) {
		this.name = name
		this.phone = phone
		this.addr = addr
	}


	
	Person.prototype.hobby = '영화감상'
	Person.prototype.info = function() {
		let msg = ''
		for ( let key in this) {
			if (key != 'info')
				msg += key + ' : ' + this[key] + '\n'
		}
		alert(msg)
	}
	//의사를 만들건데, 의사도 사람임, 기본적인 사람 객체(Person) + 의사정보(정형, 외과등등)
	function Doctor(name, phone, addr, major){
		this.name = name
		this.phone = phone
		this.addr = addr
		this.major = major
	}
	
	
	let d = new Doctor('나최고','010-3333-4444','서울시 서초구','신경외과')
	
	
	
	
	
